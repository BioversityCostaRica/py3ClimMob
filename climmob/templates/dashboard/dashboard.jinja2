                                }
                            }
                        }
                    }else {
                        all += "<div class='left'><div class='author-name'>{{ _('ClimMob Team') }} </div><div class='chat-message active'><p style='margin: 0 0 0px'>{{ "The ClimMob team invites you to give us your opinion or ask us any questions through this medium." }}</p></div></div>"
                    }

                    $("#chatDetailsMessage").html(all)
                    //$("#chatDetailsMessage").animate({ scrollTop: $("#chatDetailsMessage")[0].scrollHeight}, 1000);
                }
            </script>
